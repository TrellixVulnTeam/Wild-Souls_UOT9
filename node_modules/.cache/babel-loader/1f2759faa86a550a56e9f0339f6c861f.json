{"ast":null,"code":"import { api } from '@/config/site.config';\nimport { storageSave, storageGet } from '@/services/storage.js';\nexport default {\n  name: 'GiftCardPage',\n\n  data() {\n    return {\n      userId: 24,\n      productsCart: storageGet('cart') || [],\n      products: []\n    };\n  },\n\n  methods: {\n    addCart(id, product) {\n      let newProduct = {\n        id: product.id,\n        product_name: product.product_name,\n        product_price: product.product_price,\n        product_image: product.product_image,\n        quantity: 1\n      };\n      let currentProductsCart = storageGet('cart') || [];\n\n      if (!currentProductsCart.find(prod => prod.id === id)) {\n        let newProductsCart = [...currentProductsCart, newProduct];\n        this.productsCart = newProductsCart;\n        storageSave('cart', newProductsCart);\n      }\n    },\n\n    addAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id) {\n          currentProd.quantity = currentProd.quantity + 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n\n    decreaseAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id && currentProd.quantity > 0) {\n          currentProd.quantity = currentProd.quantity - 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n\n    removeProduct(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.splice(currentProductsCart.findIndex(function (i) {\n        return i.id === id;\n      }), 1);\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n\n    cargarProductos() {\n      api.get('products/' + this.userId + '/getAll').then(response => {\n        this.products = response.data.products;\n      });\n    }\n\n  },\n  computed: {},\n  watch: {},\n\n  created() {},\n\n  mounted() {\n    window.scrollTo({\n      top: 0,\n      behavior: 'smooth'\n    });\n    this.cargarProductos();\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\pages\\GiftCard.vue"],"names":[],"mappings":"AA+FA,SAAS,GAAT,QAAoB,sBAApB;AACA,SAAQ,WAAR,EAAqB,UAArB,QAAsC,uBAAtC;AACA,eAAe;AACb,EAAA,IAAI,EAAE,cADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,MAAM,EAAE,EADH;AAEL,MAAA,YAAY,EAAE,UAAU,CAAC,MAAD,CAAV,IAAoB,EAF7B;AAGL,MAAA,QAAQ,EAAE;AAHL,KAAP;AAKD,GARY;;AASb,EAAA,OAAO,EAAE;AACP,IAAA,OAAO,CAAC,EAAD,EAAK,OAAL,EAAa;AAClB,UAAI,UAAS,GAAI;AACf,QAAA,EAAE,EAAE,OAAO,CAAC,EADG;AAEf,QAAA,YAAY,EAAE,OAAO,CAAC,YAFP;AAGf,QAAA,aAAa,EAAE,OAAO,CAAC,aAHR;AAIf,QAAA,aAAa,EAAE,OAAO,CAAC,aAJR;AAKf,QAAA,QAAQ,EAAE;AALK,OAAjB;AAOA,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAAV,IAAoB,EAA9C;;AACA,UAAG,CAAC,mBAAmB,CAAC,IAApB,CAA0B,IAAD,IAAU,IAAI,CAAC,EAAL,KAAY,EAA/C,CAAJ,EAAuD;AACrD,YAAI,eAAc,GAAI,CAAC,GAAG,mBAAJ,EAAyB,UAAzB,CAAtB;AACA,aAAK,YAAL,GAAoB,eAApB;AACA,QAAA,WAAW,CAAC,MAAD,EAAS,eAAT,CAAX;AACF;AAED,KAhBM;;AAiBP,IAAA,SAAS,CAAC,EAAD,EAAI;AACX,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAApC;AACA,MAAA,mBAAmB,CAAC,OAApB,CAA6B,WAAD,IAAgB;AAC1C,YAAG,WAAW,CAAC,EAAZ,KAAmB,EAAtB,EAAyB;AACvB,UAAA,WAAW,CAAC,QAAZ,GAAuB,WAAW,CAAC,QAAZ,GAAuB,CAA9C;AACF;AACD,OAJD;AAKA,WAAK,YAAL,GAAoB,CAAC,GAAG,mBAAJ,CAApB;AACA,MAAA,WAAW,CAAC,MAAD,EAAS,mBAAT,CAAX;AACD,KA1BM;;AA2BP,IAAA,cAAc,CAAC,EAAD,EAAI;AAChB,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAApC;AACA,MAAA,mBAAmB,CAAC,OAApB,CAA6B,WAAD,IAAgB;AAC1C,YAAG,WAAW,CAAC,EAAZ,KAAmB,EAAnB,IAAyB,WAAW,CAAC,QAAZ,GAAuB,CAAnD,EAAqD;AACnD,UAAA,WAAW,CAAC,QAAZ,GAAuB,WAAW,CAAC,QAAZ,GAAuB,CAA9C;AACF;AACD,OAJD;AAKA,WAAK,YAAL,GAAoB,CAAC,GAAG,mBAAJ,CAApB;AACA,MAAA,WAAW,CAAC,MAAD,EAAS,mBAAT,CAAX;AACD,KApCM;;AAqCP,IAAA,aAAa,CAAC,EAAD,EAAI;AACf,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAApC;AACA,MAAA,mBAAmB,CAAC,MAApB,CACE,mBAAmB,CAAC,SAApB,CAA8B,UAAU,CAAV,EAAY;AACxC,eAAO,CAAC,CAAC,EAAF,KAAS,EAAhB;AACD,OAFD,CADF,EAIE,CAJF;AAMA,WAAK,YAAL,GAAoB,CAAC,GAAG,mBAAJ,CAApB;AACA,MAAA,WAAW,CAAC,MAAD,EAAS,mBAAT,CAAX;AACD,KA/CM;;AAgDP,IAAA,eAAe,GAAG;AAChB,MAAA,GAAG,CAAC,GAAJ,CAAQ,cAAc,KAAK,MAAnB,GAA4B,SAApC,EAA+C,IAA/C,CAAqD,QAAD,IAAc;AAChE,aAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,OAFD;AAGD;;AApDM,GATI;AAgEb,EAAA,QAAQ,EAAE,EAhEG;AAiEb,EAAA,KAAK,EAAE,EAjEM;;AAkEb,EAAA,OAAO,GAAG,CAAE,CAlEC;;AAmEb,EAAA,OAAO,GAAG;AACR,IAAA,MAAM,CAAC,QAAP,CAAgB;AAAC,MAAA,GAAG,EAAC,CAAL;AAAQ,MAAA,QAAQ,EAAE;AAAlB,KAAhB;AACA,SAAK,eAAL;AACD,GAtEY,CAuEb;AACA;AACA;;;AAzEa,CAAf","sourcesContent":["<template>\n  <div class=\"row\" style=\"background-color: #bd5b4c\">\n    <div class=\"col-12 gift-card-hero\">\n      <div class=\"row\">\n        <div class=\"col-sm-7 py-5 gift-card-title\" style=\"background-color: #ed7b49\">\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"row\" style=\"background-color: #ed7b49\">\n    <div class=\"col-12\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-sm-6\">\n            <h1 class=\"d-none d-sm-block\" style=\"font-size: 8vh;\">¡Construye tu propio Gift Box¡</h1>\n            <h1 class=\"d-block d-sm-none\">¡Construye tu propio Gift Box¡</h1>\n          </div>\n        </div>\n        <div class=\"row d-flex justify-content-center py-4\">\n          <div class=\"col-sm-3\">\n            <h2>1.</h2>\n            <p>\n              Piensa bien quién será el destinatario de la caja de regalo Wild. y elige los\n              productos en base a su Wildness!\n            </p>\n          </div>\n          <div class=\"col-sm-3\">\n            <h2>2.</h2>\n            <p>\n              Agregue a la nota lo que desea escribir en Wild Card Postal y lo haremos con nuestras\n              propias manos. ¡No olvide firmar! PD: El recibo le será enviado por correo\n              electrónico.\n            </p>\n          </div>\n          <div class=\"col-sm-3\">\n            <h2>3.</h2>\n            <p>¡Cuenta con nosotros!</p>\n            <ul class=\"p-0\">\n              <h6></h6>\n              <li class=\"d-block d-inline-block\">\n                <h6> <i class=\"bi bi-heart-fill\"></i>Wild Box</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6> <i class=\"bi bi-heart-fill\"></i>Wild Card Postal</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6> <i class=\"bi bi-heart-fill\"></i>Stickers wild</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6> <i class=\"bi bi-heart-fill\"></i>Wild box</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6> <i class=\"bi bi-heart-fill\"></i>Adornos wild</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6> <i class=\"bi bi-heart-fill\"></i>Embalaje wild</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6> <i class=\"bi bi-heart-fill\"></i>mucho cariño wild</h6>\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"row\" style=\"background-color: #ed7b49\">\n    <div class=\"col-12\">\n      <div class=\"container\">\n        <div class=\"col-3\">\n          <h2>Buscar</h2>\n        </div>\n        <div class=\"col-3\">\n          <input type=\"text\" value placeholder=\"Ingrese el producto\" class=\"px-5\">\n        </div>\n        <div class=\"row offset-md-3 d-flex align-items-start\">\n          <div v-for=\"(product, index) in products\" :key=\"index\" class=\"col-sm-12 col-md-4 py-md-5 px-4\">\n            <img :src=\"product.product_image\" class=\"img-fluid\" style=\"width: 220px; height: 240px; border-radius:275px \" />\n            <div class=\"mt-2 p-3\">\n              <h3>{{ product.product_name }}</h3>\n              <p>{{ product.product_price }} Bs.</p>\n              <button class=\"btn-product-card\" style=\"background-color: #000\">\n                <router-link :to=\"'/product/' + product.id\">Ver Producto</router-link>\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\n\nimport { api } from '@/config/site.config';\nimport {storageSave, storageGet} from '@/services/storage.js'\nexport default {\n  name: 'GiftCardPage',\n  data() {\n    return {\n      userId: 24,\n      productsCart: storageGet('cart')||[] ,\n      products: [],\n    };\n  },\n  methods: {\n    addCart(id, product){\n      let newProduct = {\n        id: product.id,\n        product_name: product.product_name,\n        product_price: product.product_price,\n        product_image: product.product_image,\n        quantity: 1,\n      };\n      let currentProductsCart = storageGet('cart')||[];\n      if(!currentProductsCart.find((prod) => prod.id === id)){\n        let newProductsCart = [...currentProductsCart, newProduct];\n        this.productsCart = newProductsCart;\n        storageSave('cart', newProductsCart)\n      }\n      \n    },\n    addAmount(id){\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach((currentProd) =>{\n        if(currentProd.id === id){\n          currentProd.quantity = currentProd.quantity + 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart)\n    },\n    decreaseAmount(id){\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach((currentProd) =>{\n        if(currentProd.id === id && currentProd.quantity > 0){\n          currentProd.quantity = currentProd.quantity - 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart)\n    },\n    removeProduct(id){\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.splice(\n        currentProductsCart.findIndex(function (i){\n          return i.id === id;\n        }),\n        1,\n      );\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart)\n    },\n    cargarProductos() {\n      api.get('products/' + this.userId + '/getAll').then((response) => {\n        this.products = response.data.products;\n      });\n    },\n\n  },\n  computed: {},\n  watch: {},\n  created() {},\n  mounted() {\n    window.scrollTo({top:0, behavior: 'smooth'});\n    this.cargarProductos();\n  },\n  // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}