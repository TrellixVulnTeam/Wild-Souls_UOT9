{"ast":null,"code":"import { api } from '@/config/site.config';\nexport default {\n  name: 'CategoriaPage',\n  props: ['id'],\n\n  data() {\n    return {\n      userId: 24,\n      products: []\n    };\n  },\n\n  methods: {\n    cargarProductos(id) {\n      api.get('categories/' + this.userId + '/products/' + id).then(response => {\n        this.products = response.data.products;\n      });\n    }\n\n  },\n  computed: {},\n  watch: {},\n\n  created() {\n    this.$watch(() => this.$route.params, (toParams, previousParams) => {\n      console.log(previousParams);\n      this.cargarProductos(toParams.id);\n    });\n  },\n\n  mounted() {\n    this.cargarProductos(this.id);\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\pages\\Categoria.vue"],"names":[],"mappings":"AAiCA,SAAS,GAAT,QAAoB,sBAApB;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,eADO;AAEb,EAAA,KAAK,EAAE,CAAC,IAAD,CAFM;;AAGb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,MAAM,EAAE,EADH;AAEL,MAAA,QAAQ,EAAE;AAFL,KAAP;AAID,GARY;;AASb,EAAA,OAAO,EAAE;AACP,IAAA,eAAe,CAAC,EAAD,EAAI;AACjB,MAAA,GAAG,CAAC,GAAJ,CAAQ,gBAAgB,KAAK,MAArB,GAA8B,YAA9B,GAA6C,EAArD,EAAyD,IAAzD,CAA+D,QAAD,IAAc;AAC5E,aAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,OAFC;AAGF;;AALO,GATI;AAgBb,EAAA,QAAQ,EAAE,EAhBG;AAiBb,EAAA,KAAK,EAAE,EAjBM;;AAkBb,EAAA,OAAO,GAAG;AACR,SAAK,MAAL,CACE,MAAM,KAAK,MAAL,CAAY,MADpB,EAEE,CAAC,QAAD,EAAW,cAAX,KAA8B;AAC5B,MAAA,OAAO,CAAC,GAAR,CAAY,cAAZ;AACA,WAAK,eAAL,CAAqB,QAAQ,CAAC,EAA9B;AACD,KALH;AAOD,GA1BY;;AA2Bb,EAAA,OAAO,GAAG;AACR,SAAK,eAAL,CAAqB,KAAK,EAA1B;AACD,GA7BY,CA+Bb;AACA;AACA;;;AAjCa,CAAf","sourcesContent":["<template>\n  \n  <div class=\"row\" style=\"background-color: #be5a4b\">\n    <div class=\"col-12 category-hero\">\n      <div class=\"row\">\n        <div class=\"col-sm-5 py-5 c