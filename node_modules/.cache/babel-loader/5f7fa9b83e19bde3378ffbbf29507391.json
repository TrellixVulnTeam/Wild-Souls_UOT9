{"ast":null,"code":"import { Swiper, SwiperSlide } from 'swiper/vue'; // Import Swiper styles\n\nimport 'swiper/css';\nimport 'swiper/css/effect-cards'; // import Swiper core and required modules\n\nimport SwiperCore, { EffectCards } from 'swiper'; // install Swiper modules\n\nSwiperCore.use([EffectCards]);\nimport { api } from '@/config/site.config';\nexport default {\n  name: 'HomePage',\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n\n  data() {\n    return {\n      mensaje: 'Hola!',\n      userId: 24,\n      idCategory1: 37,\n      idCategory2: 38,\n      products: [],\n      productsRoutes: []\n    };\n  },\n\n  methods: {\n    cargarProductos(id) {\n      api.get('categories/' + this.userId + '/products/' + id).then(response => {\n        this.products = response.data.products;\n      });\n    }\n\n  },\n  computed: {\n    loggedIn() {\n      return this.$store.state.auth.status.loggedIn;\n    }\n\n  },\n  watch: {},\n\n  created() {},\n\n  mounted() {\n    this.cargarProductos(this.idCategory1);\n    api.get('categories/' + this.userId + '/products/' + this.id).then(response => {\n      for (let x in response.data.products) {\n        this.productsRoutes.push({\n          route: '/product/' + response.data.products[x].id\n        });\n      }\n    }); //this.cargarProductos(this.idCategory2);\n  }\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\pages\\Home.vue"],"names":[],"mappings":"AAsSA,SAAS,MAAT,EAAiB,WAAjB,QAAoC,YAApC,C,CAEA;;AACA,OAAO,YAAP;AAEA,OAAO,yBAAP,C,CAEA;;AACA,OAAO,UAAP,IAAqB,WAArB,QAAwC,QAAxC,C,CAEA;;AACA,UAAU,CAAC,GAAX,CAAe,CAAC,WAAD,CAAf;AAEA,SAAS,GAAT,QAAoB,sBAApB;AACA,eAAe;AACb,EAAA,IAAI,EAAE,UADO;AAEb,EAAA,UAAU,EAAE;AACV,IAAA,MADU;AAEV,IAAA;AAFU,GAFC;;AAMb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,OAAO,EAAE,OADJ;AAEL,MAAA,MAAM,EAAE,EAFH;AAGL,MAAA,WAAW,EAAE,EAHR;AAIL,MAAA,WAAW,EAAE,EAJR;AAKL,MAAA,QAAQ,EAAE,EALL;AAML,MAAA,cAAc,EAAE;AANX,KAAP;AAQD,GAfY;;AAgBb,EAAA,OA