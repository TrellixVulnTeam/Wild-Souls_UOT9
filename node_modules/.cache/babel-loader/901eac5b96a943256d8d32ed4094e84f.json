{"ast":null,"code":"import { api } from '@/config/site.config';\nimport { storageSave, storageGet } from '@/services/storage.js';\nexport default {\n  name: 'GiftCardPage',\n\n  data() {\n    return {\n      userId: 24,\n      productsCart: storageGet('cart') || [],\n      products: [],\n      searchText: null\n    };\n  },\n\n  methods: {\n    cargarProductos() {\n      api.get('products/' + this.userId + '/getAll').then(response => {\n        this.products = response.data.products;\n      });\n    },\n\n    searchProduct() {\n      if (this.searchText !== '') {\n        let apiParams = {\n          s: this.searchText\n        };\n        api.get('products/' + this.userId + '/search', {\n          params: apiParams\n        }).then(response => {\n          this.products = response.data.products;\n        });\n      } else {\n        this.cargarProductos();\n      }\n    },\n\n    addCart(id, product) {\n      let newProduct = {\n        id: product.id,\n        product_name: product.product_name,\n        product_price: product.product_price,\n        product_image: product.product_image,\n        quantity: 1\n      };\n      let currentProductsCart = storageGet('cart') || [];\n\n      if (!currentProductsCart.find(prod => prod.id === id)) {\n        let newProductsCart = [...currentProductsCart, newProduct];\n        this.productsCart = newProductsCart;\n        storageSave('cart', newProductsCart);\n      }\n    },\n\n    addAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id) {\n          currentProd.quantity = currentProd.quantity + 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n\n    decreaseAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id && currentProd.quantity > 0) {\n          currentProd.quantity = currentProd.quantity - 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n\n    removeProduct(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.splice(currentProductsCart.findIndex(function (i) {\n        return i.id === id;\n      }), 1);\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    }\n\n  },\n  computed: {},\n  watch: {},\n\n  created() {},\n\n  mounted() {\n    window.scrollTo({\n      top: 0,\n      behavior: 'smooth'\n    });\n    this.cargarProductos();\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\pages\\GiftCard.vue"],"names":[],"mappings":"AAmIA,SAAS,GAAT,QAAoB,sBAApB;AACA,SAAS,WAAT,EAAsB,UAAtB,QAAwC,uBAAxC;AACA,eAAe;AACb,EAAA,IAAI,EAAE,cADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,MAAM,EAAE,EADH;AAEL,MAAA,YAAY,EAAE,UAAU,CAAC,MAAD,CAAV,IAAsB,EAF/B;AAGL,MAAA,QAAQ,EAAE,EAHL;AAIL,MAAA,UAAU,EAAE;AAJP,KAAP;AAMD,GATY;;AAUb,EAAA,OAAO,EAAE;AACP,IAAA,eAAe,GAAG;AAChB,MAAA,GAAG,CAAC,GAAJ,CAAQ,cAAc,KAAK,MAAnB,GAA4B,SAApC,EAA+C,IAA/C,CAAqD,QAAD,IAAc;AAChE,aAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,OAFD;AAGD,KALM;;AAMP,IAAA,aAAa,GAAG;AACd,UAAI,KAAK,UAAL,KAAoB,EAAxB,EAA4B;AAC1B,YAAI,SAAQ,GAAI;AACd,UAAA,CAAC,EAAE,KAAK;AADM,SAAhB;AAGA,QAAA,GAAG,CAAC,GAAJ,CAAQ,cAAc,KAAK,MAAnB,GAA4B,SAApC,EAA+C;AAAE,UAAA,MAAM,EAAE;AAAV,SAA/C,EAAsE,IAAtE,CAA4E,QAAD,IAAc;AACvF,eAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,SAFD;AAGF,OAPA,MAOO;AACL,aAAK,eAAL;AACF;AACD,KAjBM;;AAkBP,IAAA,OAAO,CAAC,EAAD,EAAK,OAAL,EAAc;AACnB,UAAI,UAAS,GAAI;AACf,QAAA,EAAE,EAAE,OAAO,CAAC,EADG;AAEf,QAAA,YAAY,EAAE,OAAO,CAAC,YAFP;AAGf,QAAA,aAAa,EAAE,OAAO,CAAC,aAHR;AAIf,QAAA,aAAa,EAAE,OAAO,CAAC,aAJR;AAKf,QAAA,QAAQ,EAAE;AALK,OAAjB;AAOA,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAAV,IAAsB,EAAhD;;AACA,UAAI,CAAC,mBAAmB,CAAC,IAApB,CAA0B,IAAD,IAAU,IAAI,CAAC,EAAL,KAAY,EAA/C,CAAL,EAAyD;AACvD,YAAI,eAAc,GAAI,CAAC,GAAG,mBAAJ,EAAyB,UAAzB,CAAtB;AACA,aAAK,YAAL,GAAoB,eAApB;AACA,QAAA,WAAW,CAAC,MAAD,EAAS,eAAT,CAAX;AACF;AACD,KAhCM;;AAiCP,IAAA,SAAS,CAAC,EAAD,EAAK;AACZ,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAApC;AACA,MAAA,mBAAmB,CAAC,OAApB,CAA6B,WAAD,IAAiB;AAC3C,YAAI,WAAW,CAAC,EAAZ,KAAmB,EAAvB,EAA2B;AACzB,UAAA,WAAW,CAAC,QAAZ,GAAuB,WAAW,CAAC,QAAZ,GAAuB,CAA9C;AACF;AACD,OAJD;AAKA,WAAK,YAAL,GAAoB,CAAC,GAAG,mBAAJ,CAApB;AACA,MAAA,WAAW,CAAC,MAAD,EAAS,mBAAT,CAAX;AACD,KA1CM;;AA2CP,IAAA,cAAc,CAAC,EAAD,EAAK;AACjB,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAApC;AACA,MAAA,mBAAmB,CAAC,OAApB,CAA6B,WAAD,IAAiB;AAC3C,YAAI,WAAW,CAAC,EAAZ,KAAmB,EAAnB,IAAyB,WAAW,CAAC,QAAZ,GAAuB,CAApD,EAAuD;AACrD,UAAA,WAAW,CAAC,QAAZ,GAAuB,WAAW,CAAC,QAAZ,GAAuB,CAA9C;AACF;AACD,OAJD;AAKA,WAAK,YAAL,GAAoB,CAAC,GAAG,mBAAJ,CAApB;AACA,MAAA,WAAW,CAAC,MAAD,EAAS,mBAAT,CAAX;AACD,KApDM;;AAqDP,IAAA,aAAa,CAAC,EAAD,EAAK;AAChB,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAApC;AACA,MAAA,mBAAmB,CAAC,MAApB,CACE,mBAAmB,CAAC,SAApB,CAA8B,UAAU,CAAV,EAAa;AACzC,eAAO,CAAC,CAAC,EAAF,KAAS,EAAhB;AACD,OAFD,CADF,EAIE,CAJF;AAMA,WAAK,YAAL,GAAoB,CAAC,GAAG,mBAAJ,CAApB;AACA,MAAA,WAAW,CAAC,MAAD,EAAS,mBAAT,CAAX;AACD;;AA/DM,GAVI;AA2Eb,EAAA,QAAQ,EAAE,EA3EG;AA8Eb,EAAA,KAAK,EAAE,EA9EM;;AA+Eb,EAAA,OAAO,GAAG,CAAE,CA/EC;;AAgFb,EAAA,OAAO,GAAG;AACR,IAAA,MAAM,CAAC,QAAP,CAAgB;AAAE,MAAA,GAAG,EAAE,CAAP;AAAU,MAAA,QAAQ,EAAE;AAApB,KAAhB;AACA,SAAK,eAAL;AACD,GAnFY,CAoFb;AACA;AACA;;;AAtFa,CAAf","sourcesContent":["<template>\n  <div class=\"row\" style=\"background-color: #bd5b4c\">\n    <div class=\"col-12 gift-card-hero\">\n      <div class=\"row\">\n        <div class=\"col-sm-7 py-5 gift-card-title\" style=\"background-color: #ed7b49\"></div>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"row\" style=\"background-color: #ed7b49\">\n    <div class=\"col-12\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-sm-6\">\n            <h1 class=\"d-none d-sm-block\" style=\"font-size: 8vh\">¡Construye tu propio Gift Box¡</h1>\n            <h1 class=\"d-block d-sm-none\">¡Construye tu propio Gift Box¡</h1>\n          </div>\n        </div>\n        <div class=\"row d-flex justify-content-center py-4\">\n          <div class=\"col-sm-3\">\n            <h2>1.</h2>\n            <p>\n              Piensa bien quién será el destinatario de la caja de regalo Wild. y elige los\n              productos en base a su Wildness!\n            </p>\n          </div>\n          <div class=\"col-sm-3\">\n            <h2>2.</h2>\n            <p>\n              Agregue a la nota lo que desea escribir en Wild Card Postal y lo haremos con nuestras\n              propias manos. ¡No olvide firmar! PD: El recibo le será enviado por correo\n              electrónico.\n            </p>\n          </div>\n          <div class=\"col-sm-3\">\n            <h2>3.</h2>\n            <p>¡Cuenta con nosotros!</p>\n            <ul class=\"p-0\">\n              <h6></h6>\n              <li class=\"d-block d-inline-block\">\n                <h6><i class=\"bi bi-heart-fill\"></i>Wild Box</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6><i class=\"bi bi-heart-fill\"></i>Wild Card Postal</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6><i class=\"bi bi-heart-fill\"></i>Stickers wild</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6><i class=\"bi bi-heart-fill\"></i>Wild box</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6><i class=\"bi bi-heart-fill\"></i>Adornos wild</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6><i class=\"bi bi-heart-fill\"></i>Embalaje wild</h6>\n              </li>\n              <li class=\"d-block d-inline-block\">\n                <h6><i class=\"bi bi-heart-fill\"></i>mucho cariño wild</h6>\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"row\" style=\"background-color: #ed7b49\">\n    <div class=\"col-12\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-sm-12 col-md-3\">\n            <h2>Buscar</h2>\n            <input\n              type=\"text\"\n              placeholder=\"Ingrese el producto\"\n              class=\"px-5 py-3 my-3 search\"\n              v-model=\"searchText\"\n              v-on:keyup.enter=\"searchProduct()\"\n              @keydown.enter.prevent=\"\"\n            />\n            <h2>Detalles de la orden</h2>\n            <ul class=\"p-0\">\n              <li\n                v-for=\"(prodCart, index) in productsCart\"\n                :key=\"index\"\n                class=\"d-flex align-items-center\"\n              >\n                <div>\n                  <a v-on:click.prevent=\"removeProduct(prodCart.id)\"\n                    ><i class=\"bi bi-x\" style=\"font-size: 35px\"></i\n                  ></a>\n                </div>\n                <div>\n                  <h3 class=\"my-3 mx-2\" style=\"font-size: 18px\">{{ prodCart.product_name }}</h3>\n                </div>\n                <div class=\"d-flex flex-column\">\n                  <a @click=\"addAmount(prodCart.id)\"  class=\"text-center\"><i class=\"bi bi-plus\" style=\"font-size: 30px; color:#000\"></i></a>\n                  <div class=\"amount\">{{prodCart.quantity}}</div>\n                  <a @click=\"decreaseAmount(prodCart.id)\" class=\"text-center\"><i class=\"bi bi-dash\" style=\"font-size: 30px; color:#000\"></i></a>\n                </div>\n              </li>\n\n            </ul>\n          </div>\n          <div class=\"col-sm-12 col-md-9 d-flex flex-wrap\">\n            <div v-for=\"(product, index) in products\" :key=\"index\" class=\"col-sm-12 col-md-4 px-4\">\n              <img\n                :src=\"product.product_image\"\n                class=\"img-fluid\"\n                style=\"width: 220px; height: 240px; border-radius: 275px\"\n              />\n              <div class=\"mt-2 p-3\">\n                <h3>{{ product.product_name }}</h3>\n                <p>{{ product.product_price }} Bs.</p>\n                <button\n                  @click=\"addCart(product.id, product)\"\n                  class=\"btn-product-card\"\n                  style=\"background-color: #000; color: #fff\"\n                >\n                  Agregar producto\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport { api } from '@/config/site.config';\nimport { storageSave, storageGet } from '@/services/storage.js';\nexport default {\n  name: 'GiftCardPage',\n  data() {\n    return {\n      userId: 24,\n      productsCart: storageGet('cart') || [],\n      products: [],\n      searchText: null,\n    };\n  },\n  methods: {\n    cargarProductos() {\n      api.get('products/' + this.userId + '/getAll').then((response) => {\n        this.products = response.data.products;\n      });\n    },\n    searchProduct() {\n      if (this.searchText !== '') {\n        let apiParams = {\n          s: this.searchText,\n        };\n        api.get('products/' + this.userId + '/search', { params: apiParams }).then((response) => {\n          this.products = response.data.products;\n        });\n      } else {\n        this.cargarProductos();\n      }\n    },\n    addCart(id, product) {\n      let newProduct = {\n        id: product.id,\n        product_name: product.product_name,\n        product_price: product.product_price,\n        product_image: product.product_image,\n        quantity: 1,\n      };\n      let currentProductsCart = storageGet('cart') || [];\n      if (!currentProductsCart.find((prod) => prod.id === id)) {\n        let newProductsCart = [...currentProductsCart, newProduct];\n        this.productsCart = newProductsCart;\n        storageSave('cart', newProductsCart);\n      }\n    },\n    addAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach((currentProd) => {\n        if (currentProd.id === id) {\n          currentProd.quantity = currentProd.quantity + 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n    decreaseAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach((currentProd) => {\n        if (currentProd.id === id && currentProd.quantity > 0) {\n          currentProd.quantity = currentProd.quantity - 1;\n        }\n      });\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n    removeProduct(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.splice(\n        currentProductsCart.findIndex(function (i) {\n          return i.id === id;\n        }),\n        1,\n      );\n      this.ProductsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n  },\n  computed: {\n    \n  },\n  watch: {},\n  created() {},\n  mounted() {\n    window.scrollTo({ top: 0, behavior: 'smooth' });\n    this.cargarProductos();\n  },\n  // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}