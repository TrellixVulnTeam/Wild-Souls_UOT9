{"ast":null,"code":"import { api } from '@/config/site.config';\nexport default {\n  name: 'CategoriaPage',\n  props: ['id'],\n\n  data() {\n    return {\n      userId: 24,\n      products: []\n    };\n  },\n\n  methods: {},\n  computed: {},\n  watch: {},\n\n  created() {\n    this.$watch(() => this.$route.params, (toParams, previousParams) => {\n      console.log(previousParams);\n      api.get('categories/' + this.userId + '/products/' + toParams.id).then(response => {\n        this.products = response.data.products;\n      });\n    });\n  },\n\n  mounted() {\n    api.get('categories/' + this.userId + '/products/' + this.id).then(response => {\n      this.products = response.data.products;\n    });\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\pages\\Categoria.vue"],"names":[],"mappings":"AASA,SAAS,GAAT,QAAoB,sBAApB;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,eADO;AAEb,EAAA,KAAK,EAAE,CAAC,IAAD,CAFM;;AAGb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,MAAM,EAAE,EADH;AAEL,MAAA,QAAQ,EAAE;AAFL,KAAP;AAID,GARY;;AASb,EAAA,OAAO,EAAE,EATI;AAUb,EAAA,QAAQ,EAAE,EAVG;AAWb,EAAA,KAAK,EAAE,EAXM;;AAYb,EAAA,OAAO,GAAG;AACR,SAAK,MAAL,CACE,MAAM,KAAK,MAAL,CAAY,MADpB,EAEE,CAAC,QAAD,EAAW,cAAX,KAA8B;AAC5B,MAAA,OAAO,CAAC,GAAR,CAAY,cAAZ;AACA,MAAA,GAAG,CAAC,GAAJ,CAAQ,gBAAgB,KAAK,MAArB,GAA8B,YAA9B,GAA6C,QAAQ,CAAC,EAA9D,EAAkE,IAAlE,CAAwE,QAAD,IAAc;AACnF,aAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,OAFD;AAGD,KAPH;AASD,GAtBY;;AAuBb,EAAA,OAAO,GAAG;AACR,IAAA,GAAG,CAAC,GAAJ,CAAQ,gBAAgB,KAAK,MAArB,GAA8B,YAA9B,GAA6C,KAAK,EAA1D,EAA8D,IAA9D,CAAoE,QAAD,IAAc;AAC/E,WAAK,QAAL,GAAgB,QAAQ,CAAC,IAAT,CAAc,QAA9B;AACD,KAFD;AAGD,GA3BY,CA4Bb;AACA;AACA;;;AA9Ba,CAAf","sourcesContent":["<template>\n  <div>\n    {{ id }}\n    <ul>\n      <li v-for=\"(product, index) in products\" :key=\"index\">{{ product.product_name }}</li>\n    </ul>\n  </div>\n</template>\n<script>\nimport { api } from '@/config/site.config';\n\nexport default {\n  name: 'CategoriaPage',\n  props: ['id'],\n  data() {\n    return {\n      userId: 24,\n      products: [],\n    };\n  },\n  methods: {},\n  computed: {},\n  watch: {},\n  created() {\n    this.$watch(\n      () => this.$route.params,\n      (toParams, previousParams) => {\n        console.log(previousParams);\n        api.get('categories/' + this.userId + '/products/' + toParams.id).then((response) => {\n          this.products = response.data.products;\n        });\n      },\n    );\n  },\n  mounted() {\n    api.get('categories/' + this.userId + '/products/' + this.id).then((response) => {\n      this.products = response.data.products;\n    });\n  },\n  // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}