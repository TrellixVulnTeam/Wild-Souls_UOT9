{"ast":null,"code":"import json from '../recipes.json';\nimport { api } from '@/config/site.config';\nexport default {\n  name: 'RecetaPage',\n  props: ['id'],\n\n  data() {\n    return {\n      recipe: json.recipes[this.id - 1],\n      productsInUse: []\n    };\n  },\n\n  methods: {},\n  computed: {},\n  watch: {},\n\n  created() {},\n\n  mounted() {\n    for (let x in this.recipe.recipe_products_in_use) api.get('products/getProduct/' + this.recipe.recipe_products_in_use[x]).then(response => this.productsInUse.push(response.data.product));\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\pages\\Receta.vue"],"names":[],"mappings":"AAqEA,OAAO,IAAP,MAAiB,iBAAjB;AACA,SAAS,GAAT,QAAoB,sBAApB;AACA,eAAe;AACb,EAAA,IAAI,EAAE,YADO;AAEb,EAAA,KAAK,EAAE,CAAC,IAAD,CAFM;;AAGb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,MAAM,EAAE,IAAI,CAAC,OAAL,CAAa,KAAK,EAAL,GAAQ,CAArB,CADH;AAEL,MAAA,aAAa,EAAE;AAFV,KAAP;AAID,GARY;;AASb,EAAA,OAAO,EAAE,EATI;AAYb,EAAA,QAAQ,EAAE,EAZG;AAab,EAAA,KAAK,EAAE,EAbM;;AAcb,EAAA,OAAO,GAAG,CAAE,CAdC;;AAeb,EAAA,OAAO,GAAG;AACR,SAAI,IAAI,CAAR,IAAa,KAAK,MAAL,CAAY,sBAAzB,EACE,GAAG,CAAC,GAAJ,CAAQ,yBAAyB,KAAK,MAAL,CAAY,sBAAZ,CAAmC,CAAnC,CAAjC,EAAwE,IAAxE,CAA8E,QAAD,IAAe,KAAK,aAAL,CAAmB,IAAnB,CAAwB,QAAQ,CAAC,IAAT,CAAc,OAAtC,CAA5F;AAGJ,GApBa,CAsBb;AACA;AACA;;;AAxBa,CAAf","sourcesContent":["<template>\n  <div\n    class=\"row about-us-title d-flex justify-content-center align-items-center p-md-5\"\n    style=\"background-color: #fceeea\"\n  >\n    <div class=\"col-md-6 about-us-section\">\n      <h6 class=\"text-center mt-5 pb-md-4\">{{ recipe.recipe_category }}</h6>\n      <h1 class=\"text-center pb-md-4 d-none d-sm-block\" style=\"font-size: 8vh\">\n        {{ recipe.recipe_name }}\n      </h1>\n      <h2 class=\"text-center pb-md-4 d-block d-sm-none mb-5 mt-5\">{{ recipe.recipe_name }}</h2>\n      <h6 class=\"text-center\">Tiempo de preparación: {{ recipe.recipe_preparation_time }}</h6>\n      <h6 class=\"text-center\">Porciones: {{ recipe.recipe_portions }}</h6>\n      <h6 class=\"text-center\">Dificultad: {{ recipe.recipe_difficult }}</h6>\n    </div>\n  </div>\n  <div class=\"row\" style=\"background-color: #fceeea\">\n    <div\n      class=\"col-sm-12 col-md-5 d-flex align-items-center\"\n      style=\"background-color: #f5afb9; border-top-right-radius: 250px\"\n    >\n      <div class=\"container\">\n        <div class=\"row d-flex px-md-5 mx-md-5 align-items-center justify-content-center\">\n          <div class=\"col-sm-12 col-md-12\">\n            <h6>Ingredientes</h6>\n            <ul style=\"list-style: disc\">\n              <li\n                v-for=\"(ingredient, index) in recipe.recipe_ingredients\"\n                :key=\"index\"\n                class=\"ingredient\"\n              >\n                {{ ingredient }}\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div\n      class=\"col-sm-12 col-md-7 d-flex align-items-center justify-content-center\"\n    >\n      <div class=\"container\">\n        <div class=\"col-sm-12 col-md-8 p-sm-2 p-md-5 m-md-5\">\n          <h2>Instrucciones</h2>\n          <ul style=\"list-style: disc\">\n            <li v-for=\"(instruction, index) in recipe.recipe_instructions\" :key=\"index\">\n              {{ instruction }}\n            </li>\n          </ul>\n          <h2>¡Disfruta!</h2>\n          <img :src=\"recipe.recipe_image\" class=\"img-fluid\" style=\"height: 60vh\" align=\"center\" />\n        </div>\n      </div>\n    </div>\n  <h2>Productos en uso</h2>  \n  <div v-for=\"(product, index) in productsInUse\" :key=\"index\" class=\"col-sm-12 col-md-3\">\n        \n        <img :src=\"product.product_image\" class=\"img-fluid product-card-img\" />\n        <div class=\"mt-2 p-3\">\n          <h3>{{ product.product_name }}</h3>\n          <p>{{ product.product_price }} Bs.</p>\n          <button class=\"btn-product-card\" style=\"background-color: #000\">\n            <router-link :to=\"'/product/' + product.id\">Ver Producto</router-link>\n          </button>\n        </div>\n      </div>\n  </div>\n</template>\n<script>\nimport json from '../recipes.json';\nimport { api } from '@/config/site.config';\nexport default {\n  name: 'RecetaPage',\n  props: ['id'],\n  data() {\n    return {\n      recipe: json.recipes[this.id-1],\n      productsInUse: [],\n    };\n  },\n  methods: {\n    \n  },\n  computed: {},\n  watch: {},\n  created() {},\n  mounted() {\n    for(let x in this.recipe.recipe_products_in_use)\n      api.get('products/getProduct/' + this.recipe.recipe_products_in_use[x]).then((response) => (this.productsInUse.push(response.data.product))); \n      \n      \n  }\n  \n  // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}