{"ast":null,"code":"import './assets/scss/stylesheet.scss';\nimport { api } from '@/config/site.config';\nimport { storageSave, storageGet } from '@/services/storage.js';\nexport default {\n  name: 'App',\n\n  data() {\n    return {\n      categoriesMenu: [],\n      userId: 24,\n      showMenu1: false,\n      showMenu2: false,\n      productsCart: storageGet('cart') || []\n    };\n  },\n\n  methods: {\n    logout() {\n      this.$store.dispatch('auth/logout');\n      this.$router.push('/');\n    },\n\n    showOffcanvasMenu1() {\n      this.showMenu1 ? this.showMenu1 = false : this.showMenu1 = true;\n    },\n\n    showOffcanvasMenu2() {\n      this.showMenu2 ? this.showMenu2 = false : this.showMenu2 = true;\n    },\n\n    addAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id) {\n          currentProd.quantity = currentProd.quantity + 1;\n        }\n      });\n      this.productsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n\n    decreaseAmount(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.forEach(currentProd => {\n        if (currentProd.id === id && currentProd.quantity > 0) {\n          currentProd.quantity = currentProd.quantity - 1;\n        }\n      });\n      this.productsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    },\n\n    removeProduct(id) {\n      let currentProductsCart = storageGet('cart');\n      currentProductsCart.splice(currentProductsCart.findIndex(function (i) {\n        return i.id === id;\n      }), 1);\n      this.productsCart = [...currentProductsCart];\n      storageSave('cart', currentProductsCart);\n    }\n\n  },\n  computed: {\n    loggedIn() {\n      return this.$store.state.auth.status.loggedIn;\n    },\n\n    navOptions() {\n      let navOptions = [{\n        route: '/',\n        text: 'Inicio'\n      }, {\n        route: '/sobre-nosotros',\n        text: 'Sobre nosotros'\n      }, {\n        route: '/gift-card',\n        text: 'Gift Card'\n      }, {\n        route: '/recetas',\n        text: 'Recetas'\n      }];\n\n      if (this.loggedIn) {\n        // se pueden agregar opciones extra cuando inicie sesión\n        navOptions = [...navOptions];\n      }\n\n      return navOptions;\n    },\n\n    getProductsCart() {\n      return this.productsCart;\n    }\n\n  },\n  watch: {},\n\n  mounted() {\n    api.get('categories/' + this.userId + '/getAll').then(response => {\n      for (let x in response.data.categories) {\n        this.categoriesMenu.push({\n          route: '/categoria/' + response.data.categories[x].id,\n          text: response.data.categories[x].category_name\n        });\n      }\n    });\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\App.vue"],"names":[],"mappings":"AAmLA,OAAO,+BAAP;AACA,SAAS,GAAT,QAAoB,sBAApB;AACA,SAAS,WAAT,EAAsB,UAAtB,QAAwC,uBAAxC;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,KADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,cAAc,EAAE,EADX;AAEL,MAAA,MAAM,EAAE,EAFH;AAGL,MAAA,SAAS,EAAE,KAHN;AAIL,MAAA,SAAS,EAAE,KAJN;AAKL,MAAA,YAAY,EAAE,UAAU,CAAC,MAAD,CAAV,IAAsB;AAL/B,KAAP;AAOD,GAVY;;AAWb,EAAA,OAAO,EAAE;AACP,IAAA,MAAM,GAAG;AACP,WAAK,MAAL,CAAY,QAAZ,CAAqB,aAArB;AACA,WAAK,OAAL,CAAa,IAAb,CAAkB,GAAlB;AACD,KAJM;;AAKP,IAAA,kBAAkB,GAAG;AACnB,WAAK,SAAL,GAAkB,KAAK,SAAL,GAAiB,KAAnC,GAA6C,KAAK,SAAL,GAAiB,IAA9D;AACD,KAPM;;AAQP,IAAA,kBAAkB,GAAG;AACnB,WAAK,SAAL,GAAkB,KAAK,SAAL,GAAiB,KAAnC,GAA6C,KAAK,SAAL,GAAiB,IAA9D;AACD,KAVM;;AAWP,IAAA,SAAS,CAAC,EAAD,EAAK;AACZ,UAAI,mBAAkB,GAAI,UAAU,CAAC,MAAD,CAApC;AACA,MAAA,mBAAmB,CAAC,OAApB,CAA6B,WAAD,IAAiB;AAC3C,YAAI,WAAW,CAAC,EAAZ,KAAmB,EAAvB,EAA2B;AACzB,UAAA,WAAW,CAAC,QAAZ,GAAu