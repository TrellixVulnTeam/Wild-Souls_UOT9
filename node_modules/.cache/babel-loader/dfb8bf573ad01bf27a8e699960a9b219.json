{"ast":null,"code":"import { api } from '@/config/site.config';\nexport default {\n  name: 'ProductPage',\n  props: ['id'],\n\n  data() {\n    return {\n      product: {},\n      phone: '59174913903'\n    };\n  },\n\n  methods: {\n    buyProduct() {\n      let message = '¡Hola! quiero comprar el producto: ' + this.product.product_name + ' por el precio de ' + this.product.product_price + \" Bs.\";\n      let link = 'https://api.whatsapp.com/send?phone=' + this.phone + '&text=' + encodeURIComponent(message);\n      window.location = link;\n    }\n\n  },\n  computed: {},\n  watch: {},\n\n  created() {},\n\n  mounted() {\n    window.scrollTo({\n      top: 0,\n      behavior: 'smooth'\n    });\n    api.get('products/getProduct/' + this.id).then(response => this.product = response.data.product);\n  } // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n\n\n};","map":{"version":3,"sources":["C:\\Users\\Jeloska Chavez\\proyecto final\\FRONTEND-NODE-VUE-es-boilerplate-vue\\src\\pages\\Product.vue"],"names":[],"mappings":"AA2BA,SAAS,GAAT,QAAoB,sBAApB;AAEA,eAAe;AACb,EAAA,IAAI,EAAE,aADO;AAEb,EAAA,KAAK,EAAE,CAAC,IAAD,CAFM;;AAGb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,OAAO,EAAE,EADJ;AAEL,MAAA,KAAK,EAAE;AAFF,KAAP;AAMD,GAVY;;AAWb,EAAA,OAAO,EAAE;AACP,IAAA,UAAU,GAAE;AACV,UAAI,OAAM,GAAI,wCAAwC,KAAK,OAAL,CAAa,YAArD,GAAoE,oBAApE,GAA2F,KAAK,OAAL,CAAa,aAAxG,GAAwH,MAAtI;AACA,UAAI,IAAG,GAAI,yCAAyC,KAAK,KAA9C,GAAsD,QAAtD,GAAiE,kBAAkB,CAAC,OAAD,CAA9F;AACA,MAAA,MAAM,CAAC,QAAP,GAAkB,IAAlB;AACD;;AALM,GAXI;AAmBb,EAAA,QAAQ,EAAE,EAnBG;AAoBb,EAAA,KAAK,EAAE,EApBM;;AAqBb,EAAA,OAAO,GAAG,CAAE,CArBC;;AAsBb,EAAA,OAAO,GAAG;AACR,IAAA,MAAM,CAAC,QAAP,CAAgB;AAAC,MAAA,GAAG,EAAC,CAAL;AAAQ,MAAA,QAAQ,EAAE;AAAlB,KAAhB;AACA,IAAA,GAAE,CACC,GADH,CACO,yBAAyB,KAAK,EADrC,EAEG,IAFH,CAES,QAAD,IAAe,KAAK,OAAL,GAAe,QAAQ,CAAC,IAAT,CAAc,OAFpD;AAGD,GA3BY,CA4Bb;AACA;AACA;;;AA9Ba,CAAf","sourcesContent":["<template>\n<div class=\"row\" style=\"background-color:#fee799\">\n  <div class=\"col-12\">\n    <div class=\"container\" >\n    <div class=\"row d-flex align-items-center m-5 product-section\">\n      <div class=\"col-md-5\">\n        <h1>{{ product.product_name }}</h1>\n        <p>{{product.product_description}}</p>\n      </div>\n      <div class=\"col-md-4\">\n        <img :src=\"product.product_image\" class=\"product-img-info img-fluid\" />\n      </div>\n      <div class=\"col-md-3\" align=\"center\">\n        <h2 class=\"text-right\">{{ product.product_price }} Bs.</h2>\n        <button class=\"btn-product-buy m-2\" align=\"center\" @click=\"buyProduct()\">\n          Comprar ahora\n        </button>\n        \n      </div>\n    </div>\n  </div>\n  </div>\n  \n</div>\n  \n</template>\n<script>\nimport { api } from '@/config/site.config';\n\nexport default {\n  name: 'ProductPage',\n  props: ['id'],\n  data() {\n    return {\n      product: {},\n      phone: '59174913903',\n\n\n    };\n  },\n  methods: {\n    buyProduct(){\n      let message = '¡Hola! quiero comprar el producto: ' + this.product.product_name + ' por el precio de ' + this.product.product_price + \" Bs.\";\n      let link = 'https://api.whatsapp.com/send?phone=' + this.phone + '&text=' + encodeURIComponent(message);\n      window.location = link;\n    },\n    \n  },\n  computed: {},\n  watch: {},\n  created() {},\n  mounted() {\n    window.scrollTo({top:0, behavior: 'smooth'});\n    api\n      .get('products/getProduct/' + this.id)\n      .then((response) => (this.product = response.data.product));\n  },\n  // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}