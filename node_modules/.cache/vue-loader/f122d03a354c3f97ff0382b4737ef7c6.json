e=\"{ visibility: showMenu ? 'visible' : 'hidden'}\"\n    >\n    <div align=\"right\">\n      <button\n          class=\"btn-close text-reset\"\n          @click.prevent=\"showOffcanvasMenu()\"\n          \n        ></button>\n    </div>\n     \n      <div class=\"offcanvas-header\">\n        <h5 class=\"offcanvas-title m-3\" id=\"\">categorías</h5>\n       \n      </div>\n      <div class=\"offcanvas-body  mx-3 mb-3\">\n        <template v-for=\"(navOption, index) in categoriesMenu\" :key=\"index\">\n            <div class=\"mb-2\">\n              <h3>\n                <a>\n                <router-link\n                  class=\"categories-menu-item\"\n                  :class=\"this.$route.path === navOption.route ? 'active' : ''\"\n                  :to=\"navOption.route\"\n                  @click.prevent=\"showOffcanvasMenu()\"\n                >\n                  {{ navOption.text }}\n                </router-link>\n              </a>\n              </h3>\n              \n            </div>\n          </template>\n      </div>\n    </div>\n    <div class=\"container-fluid\">\n      <div class=\"col-md-12\">\n        <div>\n          <router-view></router-view>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"container-fluid\" style=\"background-color: #be5a4b\">\n    <div class=\"col-12\">\n      <div class=\"container-fluid\">\n        <div class=\"row\" align=\"center\">\n          <img src=\"./assets/images/logo.svg\" class=\"my-5\" height=\"200\" />\n        </div>\n        <div class=\"row d-flex d-flex justify-content-center\">\n          <div class=\"col-md-4 col-xl-5\">\n            <div class=\"d-none d-xl-block mb-lg-5\">\n              <h1>¡Mantente Wild!</h1>\n              <div class=\"d-flex align-items-center\">\n                <form>\n                  <input class=\"input-email\" placeholder=\"Escribe tu email\" type=\"email\" />\n                  <a><i class=\"bi bi-arrow-right-circle-fill send-mail\"></i></a>\n                </form>\n              </div>\n            </div>\n            <div class=\"d-none d-xl-block mb-lg-5\">\n              <h2>Síguenos</h2>\n              <ul class=\"social-media-list\">\n                <li>\n                  <a><i class=\"bi bi-facebook\"></i></a>\n                </li>\n                <li>\n                  <a><i class=\"bi bi-instagram\"></i></a>\n                </li>\n              </ul>\n            </div>\n          </div>\n          <div class=\"col-md-4 offset-xl-1 col-xl-3 m-lg-0\">\n            <div class=\"d-none d-xl-block mb-lg-5\">\n              <h2>Productos</h2>\n              <ul class=\"list-footer-products\">\n                <li><a href=\"#\">Mantequilla de sésamo/tahini</a></li>\n                <li><a href=\"#\">Mantequilla de nuez</a></li>\n                <li><a href=\"#\">Miel</a></li>\n                <li><a href=\"#\">Mermeladas</a></li>\n                <li><a href=\"#\">Halva</a></li>\n                <li><a href=\"#\">Frutos secos</a></li>\n              </ul>\n            </div>\n          </div>\n          <div class=\"col-md-4 col-xl-3\">\n            <div class=\"d-none d-xl-block mb-lg-5\">\n              <h2>Contactanos</h2>\n              <ul class=\"list-footer-products\">\n                <li><a href=\"#\">wildsouls@info.com</a></li>\n                <li><a href=\"#\">+591-74913903</a></li>\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport './assets/scss/stylesheet.scss';\nimport { api } from '@/config/site.config';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      categoriesMenu: [],\n      userId: 24,\n      showMenu: false,\n    };\n  },\n  methods: {\n    logout() {\n      this.$store.dispatch('auth/logout');\n      this.$router.push('/');\n    },\n    showOffcanvasMenu() {\n      this.showMenu ? (this.showMenu = false) : (this.showMenu = true);\n    },\n  },\n  computed: {\n    loggedIn() {\n      return this.$store.state.auth.status.loggedIn;\n    },\n    navOptions() {\n      let navOptions = [\n        {\n          route: '/',\n          text: 'Inicio',\n        },\n        {\n          route: '/sobre-nosotros',\n          text: 'Sobre nosotros',\n        },\n        {\n          route: '/gift-card',\n          text: 'Gift Card',\n        },\n        {\n          route: '/recetas',\n          text: 'Recetas',\n        },\n      ];\n      if (this.loggedIn) {\n        // se pueden agregar opciones extra cuando inicie sesión\n        navOptions = [...navOptions];\n      }\n      return navOptions;\n    },\n  },\n  watch: {},\n  mounted() {\n    api.get('categories/'+this.userId+'/getAll').then((response)=> {\n      for(let x in response.data.categories){\n        this.categoriesMenu.push({\n          route: '/categoria/' + response.data.categories[x].id,\n          text: response.data.categories[x].category_name,\n        });\n      }\n    });\n  },\n  // Se pueden utilizar estos hooks para el ciclo de vida\n  // beforeCreate, created, beforeMount, mounted, beforeUpdate, updated\n  // activated, deactivated, beforeUnmount, unmounted\n};\n</script>\n\n<style scoped></style>\n"]}]}